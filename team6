    <script type="text/javascript">
	var color=new Array("梅花","黑桃","方塊","紅心");
	var num=new Array('A','2','3','4','5','6','7','8','9','J','Q','K');
	
	var i=0;
	var j=0;
	for(;i<4;i++){
		for(j in num){
			console.log(color[i]+num[j]);
		}
	}
	console.log("/* Shuffling cards!!*/");
	
	var color_rnd ;
	var num_rnd ;
	
	for(var k = 0 ; k<5 ; k++){
		color_rnd = Math.floor(Math.random()*3);
		num_rnd = Math.floor(Math.random()*10); 
		
		console.log(color[color_rnd]+num[num_rnd]);
	}
	
	
    </script>
